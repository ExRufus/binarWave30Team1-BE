// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id          String @id @default(uuid())
  Email       String @unique
  Username    String
  Password    String
  Total_score Int
  Biodata     String
  City        String
}

model game {
  id            String @id @default(uuid())
  Name          String
  Description   String
  thumbnail_url String
  Game_url      String
  play_count    Int
}
